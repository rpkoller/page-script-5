{
  "manifest_version": 3,
  "name": "__MSG_extension_name_chrome__",
  "version": "1.1",
  "description": "__MSG_extension_description__",
  "homepage_url": "https://skipto-landmarks-headings.github.io/page-script-5/",
  "default_locale": "en",
  "icons": {
    "16": "./icons/skipto-icon-16.png",
    "32": "./icons/skipto-icon-32.png",
    "48": "./icons/skipto-icon-48.png",
    "64": "./icons/skipto-icon-64.png",
    "128": "./icons/skipto-icon-128.png"
  },
  "action": {
    "default_title": "Toggle Menu"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "web_accessible_resources": [{
    "resources": ["skipto.js"],
    "matches": ["<all_urls>"]
  }],
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
