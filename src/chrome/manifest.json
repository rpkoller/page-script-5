{
  "manifest_version": 3,
  "name": "Skip To Content for WCAG 2.4.1 ByPass Blocks",
  "version": "1.0",
  "description": "Skip To Content provides keyboard navigation to landmark regions and headings to meet WCAG 2.4.1 Bypass Blocks requirement.",
  "icons": {
    "16": "./images/skipto-icon-16.png",
    "32": "./images/skipto-icon-32.png",
    "48": "./images/skipto-icon-48.png",
    "64": "./images/skipto-icon-64.png",
    "128": "./images/skipto-icon-128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "web_accessible_resources": [{
    "resources": ["skipto.js"],
    "matches": ["<all_urls>"]
  }],
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
